class Cell{
protected:
  bool m_active; // 1 = active; 0 = quiescent
  double m_p; //probAct
  double m_alpha; //prolifRate
  double m_beta; //quies2ActTim
  double m_GammaAct; //actDeathRate
  double m_GammaQuie; //quieDeathRate
public:
  Cell(bool active, double p, double alpha, double beta,
       double GammaAct, double GammaQuie);
  int updateCell(vector<bool> &newCell, bool &killCell);
};
